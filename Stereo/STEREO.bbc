   10  MODE1
   20  VDU 23,240,0,0,0,255,0,255,0,255
   30  VDU5
   35  FOR X%=0 TO 700 STEP 128
   40  FOR Y%=200 TO RND(1000)+200 STEP 26
   60  MOVE X%,Y%
   65  IF X%=0 OR X%=640 THEN GCOL0,1
   70  PRINT CHR$(240)
   75  IF X%=0 OR X%=640 THEN GCOL0,2
   90  NEXT Y%
   95  NEXT X%
  100  MOVE 246,70
  110  PRINT "STEREO"
  120  VDU 4
  130  COLOUR 1
  140  PRINT TAB(0,27);"L";SPC(19);"R"
  150  COLOUR 6
  160  PRINT TAB(4,27);"1   2   3   4"
  170  COLOUR 7
